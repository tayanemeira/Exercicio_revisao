num1 = int(input("diga um numero "))
num2 = int(input("diga um numero "))
num3 = int(input("diga um numero "))
if num1 > num2:
    if num1>num3:
        print (f"o numero {num1} é maior")
    else:
        print (f"o numero {num3} é maior")
elif num2 > num3:
    print (f"o numero {num2} é maior")
else:
    print (f"o numero {num3} é maior")
